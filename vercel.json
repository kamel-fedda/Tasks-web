
{
    "builds": [{
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
        "buildCommand": "python manage.py collectstatic --noinput"
        }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
            
        }
    ]
}
